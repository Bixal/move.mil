{
  "name": "move.mil-uswds-subtheme",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Bixal/move.mil.git"
  },
  "license": "ISC",
  "scripts": {
    "build": "gulp build",
    "vis-ref": "backstop reference --configPath=backstop-settings --pathfile=backstop-paths",
    "vis-test": "backstop test --configPath=backstop-settings --pathfile=backstop-paths",
    "screenshots:local": "wdio wdio.conf.js",
    "build-css": "gulp sass",
    "build-js": "gulp javascript",
    "build-img": "gulp images",
    "build-fonts": "gulp fonts",
    "clean": "gulp clean-assets",
    "watch": "nswatch",
    "lint": "npm run lint-js lint-scss",
    "lint-js": "gulp jshint",
    "lint-scss": "gulp lint-scss"
  },
  "watch": {
    "./scss/**/*.scss": "lint-scss!build-css",
    "./images/**/*": [
      "build-img"
    ],
    "./js/**/*.js": "lint-js!build-js",
    "./node_modules/uswds/src/stylesheets": [
      "build-css"
    ],
    "./node_modules/uswds/src/fonts": [
      "build-fonts"
    ],
    "./node_modules/uswds/src/img": [
      "build-img"
    ],
    "./node_modules/uswds/src/js": [
      "build-js"
    ]
  },
  "dependencies": {
    "autoprefixer": "^8.6.5",
    "browserify": "^16.2.3",
    "chalk": "^2.4.2",
    "check-dependencies": "^1.1.0",
    "del": "^3.0.0",
    "gulp": "^4.0.0",
    "gulp-clean": "^0.4.0",
    "gulp-concat": "^2.6.1",
    "gulp-cssnano": "^2.1.3",
    "gulp-eslint": "^4.0.2",
    "gulp-jshint": "^2.1.0",
    "gulp-postcss": "^7.0.1",
    "gulp-rename": "^1.4.0",
    "gulp-sass": "^4.0.2",
    "gulp-sass-glob": "^1.0.8",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-stylelint": "^7.0.0",
    "gulp-uglify": "^3.0.2",
    "gulp-util": "^3.0.8",
    "jshint": "^2.10.2",
    "jshint-stylish": "^2.2.1",
    "merge-stream": "^1.0.1",
    "node-notifier": "^5.4.0",
    "nswatch": "^0.2.0",
    "run-sequence": "^2.2.1",
    "slick-carousel": "^1.8.1",
    "stylelint": "^9.10.1",
    "stylelint-config-recommended": "^2.1.0",
    "stylelint-scss": "^2.5.0",
    "uswds": "^1.6.12",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0"
  },
  "devDependencies": {
    "backstopjs": "^3.8.8",
    "casperjs": "^1.1.4",
    "minimalist": "^1.0.0",
    "minimist": "^1.2.0",
    "path": "^0.12.7",
    "slimerjs": "^1.0.0"
  }
}
